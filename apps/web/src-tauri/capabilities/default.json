{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for Urban Home School desktop app",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-set-title",
    "core:app:default",
    "core:path:default",
    "core:event:default",
    "core:webview:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "http://localhost:8000/**" },
        { "url": "https://*.urbanhomeschool.ke/**" },
        { "url": "https://fonts.googleapis.com/**" },
        { "url": "https://fonts.gstatic.com/**" },
        { "url": "https://models.readyplayer.me/**" }
      ]
    },
    "updater:default",
    "deep-link:default",
    "store:default"
  ]
}
